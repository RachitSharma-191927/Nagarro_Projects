<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>HTML Form</title>
    <link
      rel="stylesheet"
      href="	https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css"
    />
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <div id="contain">
      <div id="header">
        <h4>Movie Addition</h4>
      </div>
      <div class="mData">
        <label for="mName">Movie Name</label>
        <input type="text" id="mName" placeholder="Enter Movie Name" />
      </div>
      <div class="mData">
        <label for="mGenre">Movie Genre</label>
        <input type="text" id="mGenre" placeholder="Enter Movie Genre" />
      </div>
      <div class="mData">
        <label for="mYear">Movie Year</label>
        <input type="text" id="mYear" placeholder="Enter Movie Year" />
      </div>

      <div class="btns">
        <button type="submit" class="btn btn-primary" id="get">Get</button>
        <button type="submit" class="btn btn-primary" id="post">Post</button>
      </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="script.js"></script>
  </body>
</html>
